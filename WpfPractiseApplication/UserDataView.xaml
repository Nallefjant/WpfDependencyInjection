<UserControl x:Class="WpfPractiseApplication.UserDataView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfPractiseApplication"
             mc:Ignorable="d" >
    <StackPanel >
        <TextBox x:Name="textBox" Text="{Binding Name, Mode=OneWay}" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="Aqua" />
    </StackPanel>
</UserControl>
